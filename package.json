{
    "name": "@nftperp/sdk",
    "version": "3.8.0",
    "description": "SDK for nftperp protocol",
    "main": "sdk.js",
    "types": "sdk.d.ts",
    "scripts": {
        "build": "tsc",
        "start": "node dist/sdk.js",
        "prepare": "husky install",
        "typechain": "typechain --target ethers-v5 --out-dir src/typechain-types 'src/abis/**/*.json'",
        "postinstall": "npm run typechain"
    },
    "homepage": "https://github.com/nftperp/nftperp-sdk/#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/nftperp/nftperp-sdk.git"
    },
    "keywords": [
        "sdk",
        "nft",
        "nfts",
        "nftperp"
    ],
    "author": "Yash M <ymehta777@gmail.com>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/nftperp/nftperp-sdk/issues"
    },
    "engines": {
        "node": ">=14"
    },
    "dependencies": {
        "axios": "^1.1.3",
        "big.js": "^6.2.1",
        "ethers": "^5.6.9",
        "socket.io-client": "^4.6.1"
    },
    "devDependencies": {
        "@typechain/ethers-v5": "^10.1.0",
        "@types/big.js": "^6.1.5",
        "@typescript-eslint/eslint-plugin": "^5.32.0",
        "@typescript-eslint/parser": "^5.32.0",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "prettier": "^2.7.1",
        "typechain": "^8.1.0",
        "typescript": "^4.7.4"
    }
}
