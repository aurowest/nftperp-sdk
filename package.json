{
    "name": "@nftperp/sdk",
    "version": "4.3.0",
    "description": "SDK for nftperp protocol",
    "main": "sdk.js",
    "types": "sdk.d.ts",
    "scripts": {
        "build": "tsc",
        "start": "node dist/sdk.js",
        "prepare": "husky install",
        "typechain": "typechain --target ethers-v6 --out-dir src/typechain-types 'src/abis/**/*.json'",
        "postinstall": "npm run typechain"
    },
    "homepage": "https://github.com/nftperp/nftperp-sdk/#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/nftperp/nftperp-sdk.git"
    },
    "keywords": [
        "sdk",
        "nft",
        "nfts",
        "nftperp"
    ],
    "author": "Yash M <ymehta777@gmail.com>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/nftperp/nftperp-sdk/issues"
    },
    "engines": {
        "node": ">=14"
    },
    "dependencies": {
        "axios": "^1.6.4",
        "big.js": "^6.2.1",
        "ethers": "^6.9.2"
    },
    "devDependencies": {
        "@typechain/ethers-v6": "^0.5.1",
        "@types/big.js": "^6.2.2",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "prettier": "^3.1.1",
        "typechain": "^8.3.2",
        "typescript": "^5.3.3"
    }
}
